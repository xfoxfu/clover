    "protocol": "vmess",
    "settings": {
      "vnext": [{
        "address": "example.com",
        "port": 443,
        "users": []
      }]
    },
    "mux": {
      "enabled": true,
      "concurrency": 8
    },
    "streamSettings": {
      "network": "ws",
      "security": "tls",
      "tlsSettings": {
        "serverName": "example.com",
        "allowInsecure": false
      },
      "wsSettings": {
        "path": "/",
        "headers": {
          "Host": "example.com"
        }
      }
    }
  },
  "outboundDetour": [{
